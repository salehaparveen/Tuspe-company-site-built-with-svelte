{
  "name": "tuspe",
  "version": "2.0.0",
  "description": "Official website of Tuspe Design Oy built in Svelle and Routify.",
  "homepage": "https://tuspe.com",
  "bugs": "https://github.com/timoanttila/Svelte-Tuspe/issues",
  "license": "GNU GPL",
  "keywords": [
    "svelte",
    "routify",
    "svelte-framework",
    "svelte-example",
    "svelte-template",
    "prerendered",
    "svelte-prerendered",
    "rollup",
    "website",
    "spa",
    "pwa"
  ],
  "author": {
    "name": "Timo Anttila",
    "email": "timo@tuspe.com",
    "url": "https://timoanttila.com"
  },
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "routify -b && rollup -c && routify export && workbox injectManifest",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "build:docker": "npm run build && ./scripts/docker/docker-build.sh",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "nollup": "^0.15.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.40.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-hot": "^0.13.0",
    "rollup-plugin-terser": "^7.0.2",
    "spassr": "^2.2.3"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.4.1",
    "@sveltech/routify": "^1.9.10",
    "svelte": "^3.35.0",
    "workbox-cli": "^6.1.1"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true
  }
}
